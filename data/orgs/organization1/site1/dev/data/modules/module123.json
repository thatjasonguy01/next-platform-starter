        "data-type": "module",
        "class": "module"
    },
    "container1": {
        "data-type": "container",
        "class": "container"
    },
    "row1": {
        "data-type": "row",
        "class": "row"
    },
    "column1": {
        "data-type": "column",
        "class": "col-3"
    },
    "link1": {
